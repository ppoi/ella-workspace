{
  "name": "ella",
  "dockerComposeFile": "./compose.yml",
  "service": "workspace",
  "workspaceFolder": "/workspaces",
  "customizations": {
    "vscode": {
      "extensions": [
        "svelte.svelte-vscode",
        "antfu.vite",
        "redhat.vscode-yaml",
        "vscjava.vscode-java-pack",
        "redhat.vscode-quarkus",
        "vscjava.vscode-maven"
      ],
      "settings": {
        "git.defaultCloneDirectory": "/workspaces"
      }
    }
  }
}